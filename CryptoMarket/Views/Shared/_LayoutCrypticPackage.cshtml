        <script src="~/Assets/cryptic_package/plugins/amcharts/depthChart/export.min.js"></script>
        <script src="~/Assets/cryptic_package/plugins/amcharts/depthChart/light.js"></script>
        <script src="~/Assets/cryptic_package/js/charts-amcharts.js"></script>
        <!-- Custom Theme Scripts -->
        <script src="~/Assets/cryptic_package/js/custom.min.js"></script>
        <script src="~/Assets/cryptic_package/js/preloader.min.js"></script>
    <!-- Application Scripts -->
    @RenderSection("scripts", false)
    
    <script src="~/Assets/notiny/notiny.min.js"></script>
